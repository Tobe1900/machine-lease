}

.days_flex {
  flex: 1;
  background-color: #fff;
  color: #888;
  font-size: 14px;
  padding-top: 10px;
}

.days_flex_text {
  height: 24px;
  line-height: 24px;
  width: 100%;
  display: inline-block;
  border-bottom: 2px solid #ccc;
}

.picker_default {
  display: flex;
  flex-direction: column;
  font-size: 12px;
  color: #888;
}

.picker_actived {
  display: flex;
  flex-direction: column;
  font-size: 12px;
  color: #888;
}

.close_svg {
  background-image: url("data:image/svg+xml,%3Csvg t='1580618393306' class='icon' viewBox='0 0 1024 1024' version='1.1' xmlns='http://www.w3.org/2000/svg' p-id='5192' width='20' height='20'%3E%3Cpath d='M810.666667 273.493333L750.506667 213.333333 512 451.84 273.493333 213.333333 213.333333 273.493333 451.84 512 213.333333 750.506667 273.493333 810.666667 512 572.16 750.506667 810.666667 810.666667 750.506667 572.16 512z' p-id='5193' fill='%238a8a8a'%3E%3C/path%3E%3C/svg%3E");
  background-position: top center;
  background-repeat: none;
  height: 20px;
  width: 20px;
}

.choose_days_container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.confirm_btn_container {
  width: 100%;
  padding: 0 10px 10px 10px;
}

.confirm_btn {
  height: 40px;
  line-height: 40px;
  color: #fff;
  background-color: #1296db;
}

.day_text {
  width: 30%;
  height: 36px;
  border: 1px solid #1296db;
  line-height: 36px;
  margin: 3px;
  color: #1296db;
}

.rent_container {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-top: 20rpx;
}

.rent_item {
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f8f8f8;
  height: 40px;
  line-height: 40px;
  font-size: 28rpx;
}

.item_label {
  flex: 1;
  text-align: left;
  color: #666;
  padding-left: 8px;
}

.item_value {
  flex: 1;
  text-align: right;
  padding-right: 8px;
}

.rent_rule_desc {
  text-align: left;
  padding: 12rpx;
  color: #444;
  font-size:24rpx;
}

.rent_rule_label {
  background-image: url("data:image/svg+xml,%3Csvg t='1581194736611' class='icon' viewBox='0 0 1024 1024' version='1.1' xmlns='http://www.w3.org/2000/svg' p-id='6817' width='18' height='18'%3E%3Cpath d='M512 0a512 512 0 1 1 0 1024A512 512 0 0 1 512 0z m0 861.888a68.288 68.288 0 1 0 0-136.576 68.288 68.288 0 0 0 0 136.576z m186.56-379.648c29.952-43.904 41.088-82.304 40.96-136.192-0.128-84.864-80.256-174.976-200.96-174.976-128.64 0-253.696 63.744-253.696 227.2l113.28 0.384c0-55.808 36.032-114.944 113.024-114.944 46.08 0 112.64 18.624 112.64 86.72 0 62.656-20.48 75.456-81.92 120.32-59.328 40.32-89.6 92.992-87.424 158.08v33.664h115.584c0-102.912 72.768-115.392 128.512-200.32z' p-id='6818' fill='%231296db'%3E%3C/path%3E%3C/svg%3E");
  background-position: 55px center;
  background-repeat: no-repeat;
}

.radio {
  margin-left: 20rpx;
  margin-right: 24rpx;
}
