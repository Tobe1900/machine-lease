.dashed_container {
  text-align: center;
  padding: 15rpx;
  border: 2px dashed #dedede;
  color: #666;
  margin: 40rpx;
  background-color: #f8f8f8;
}

.address_container{
  flex:3;
  display:flex;
  flex-direction:column;
  padding-top: 2px;
}

.address_container text:nth-child(2){
  font-size: 24rpx;
  color:#888;
}

.flex_container {
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f8f8f8;
}

.flex_image {
  width: 120px;
  height: 100px;
  flex: 1;
}

.flex_name {
  display: flex;
  flex-direction: column;
  flex: 2;
  justify-content: center;
  font-size: 28rpx;
  padding-left: 8px;
}

.flex_text {
  height: 36p;
  line-height: 36px;
}

.flex_num {
  flex: 1;
  justify-content: center;
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  text-align: center;
  color: #999;
}

.container {
  padding-top: 8px;
  background-color: #f8f8f8;
}

.use_days_container {
  background-color: #f8f8f8;
  display: flex;
  flex-direction: row;
  text-align: center;
  border-bottom: 1px solid #fefefe;
  height: 75px;
  padding-bottom: 8px;
}

.use_days_flex {
  flex: 1;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  padding-top: 12px;
  background-image: url("data:image/svg+xml,%3Csvg t='1580713023490' class='icon' viewBox='0 0 1024 1024' version='1.1' xmlns='http://www.w3.org/2000/svg' p-id='2661' width='18' height='18'%3E%3Cpath d='M346.112 478.549333v136.533334h-136.533333v-136.533334zM580.266667 478.549333v136.533334H443.733333v-136.533334zM815.104 478.549333v136.533334h-136.533333v-136.533334zM346.112 676.522667v136.533333h-136.533333v-136.533333zM580.266667 676.522667v136.533333H443.733333v-136.533333zM815.104 676.522667v136.533333h-136.533333v-136.533333zM209.578667 0h136.533333v318.805333h-136.533333z' p-id='2662' fill='%23888888'%3E%3C/path%3E%3Cpath d='M209.578667 0h136.533333v318.805333h-136.533333zM678.570667 0h136.533333v318.805333h-136.533333z' p-id='2663' fill='%23888888'%3E%3C/path%3E%3Cpath d='M678.570667 0h136.533333v318.805333h-136.533333zM209.578667 206.848h136.533333v111.957333h-136.533333z' p-id='2664' fill='%23888888'%3E%3C/path%3E%3Cpath d='M678.570667 206.848h136.533333v111.957333h-136.533333z' p-id='2665' fill='%23888888'%3E%3C/path%3E%3Cpath d='M830.122667 204.8v148.138667H663.552V204.8H360.448v148.138667H194.56V204.8H34.133333v819.2h955.733334V204.8zM853.333333 894.293333H170.666667V430.762667h682.666666z' p-id='2666' fill='%23888888'%3E%3C/path%3E%3C/svg%3E");
